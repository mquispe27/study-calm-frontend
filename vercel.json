{
  "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }],
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/uploads/(.*)",
      "dest": "/api/uploads/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}